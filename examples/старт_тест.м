взяти доля [Доля, ТестовийВипадок]

Доля(тестові_випадки=[
  ТестовийВипадок(
    назва="Тестування вірного твердження (1 рівно 1)",
    сценарій=дія(я)
      я.вірно(1 рівно 1)
    кінець
  ),
  ТестовийВипадок(
    назва="Тестування невірного твердження (2 * 2 рівно 2)",
    сценарій=дія(я)
      я.вірно(2 * 2 рівно 2)
    кінець
  ),
  ТестовийВипадок(
    назва="Пустий тестовий випадок є успішний",
    сценарій=дія(я)
      вернути пусто
    кінець
  ),
  ТестовийВипадок(
    назва="Провалити тестовий випадок самостійно",
    сценарій=дія(я)
      я.провалити("Цей тестовий випадок повинен провалитись")
    кінець
  )
]).старт()
